# Supabase
NEXT_PUBLIC_SUPABASE_URL=your-supabase-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# Google Maps
# Client-side key (for Places Autocomplete and Maps display)
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your-google-maps-api-key
# Server-side key (for Directions and Distance Matrix API - optional, uses client key if not set)
GOOGLE_MAPS_SERVER_API_KEY=your-google-maps-server-api-key

